<?xml version="1.0" encoding="UTF-8"?>

<vddr-enables>

    <vddr-enable>
        <centaur-target><type>membuf</type><node>0</node><position>0</position></centaur-target>
        <i2c>
            <master-target><type>membuf</type><node>0</node><position>0</position></master-target>
            <engine>1</engine>
            <port>0</port>
            <address>0xC0</address>
        </i2c>
        <io-expander>
            <part-type>PCA9535</part-type>
            <port>0</port>         <!-- port # on the expander -->
            <pin>0</pin>           <!-- pin # on the port -->
            <polarity>1</polarity> <!-- 1 = set high to enable VDDR -->
        </io-expander>
    </vddr-enable>

</vddr-enables>
